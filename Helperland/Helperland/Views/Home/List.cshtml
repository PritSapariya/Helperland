            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BankTokenId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TaxNo)
